[push]
  default = current
[user]
	name = Yves Le Douaron
	email = yledouaron@h2ad.net
[color]
	ui = true
[help]
	autocorrect = 1
[core]
	autocrlf = input
	editor = vim
	excludesfile = ~/.cvsignore
[merge]
	tool = vimdiff
[man]
	viewer = most
[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	last = log -1 HEAD
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[diff]
	tool = vimdiff
